<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.poo.controllers.SecondaryController">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>

    <Label text="Nombre:"/>
    <TextField fx:id="nameField" promptText="Ingrese el nombre"/>
    
    <Label text="ID:"/>
    <TextField fx:id="idField" promptText="Ingrese el ID"/>

    <Button text="Crear Administrador" onAction="#handleCreateAdmin"/>
</VBox>