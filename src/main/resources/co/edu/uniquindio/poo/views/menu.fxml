<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.poo.controllers.MenuController">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>

    <Button text="Agregar Deporte" onAction="#handleAddSport"/>

    <Button text="Agregar Entrenador" onAction="#handleAddTrainer"/>

    <Button text="Agregar Miembro" onAction="#handleAddMember"/>

    <Button text="Agregar Sesion" onAction="#handleAddSession"/>

    <Button text="Gestionar Sesiones" onAction="#handleManageSessions"/>
</VBox>